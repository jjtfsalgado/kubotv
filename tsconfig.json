{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "lib": [
      "dom",
      "es7"
    ],
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "includes": [
    "./externals.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}